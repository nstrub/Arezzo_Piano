<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>

<MenuBar xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="arezzo.vues.VueMenu">
  <menus>
    <Menu mnemonicParsing="false" text="Menu">
      <items>
        <MenuItem mnemonicParsing="false" onAction="#quitter" text="Quitter" />
          <MenuItem fx:id="nouveau" mnemonicParsing="false" onAction="#nouveau" text="Nouveau" />
      </items>
    </Menu>
      <Menu mnemonicParsing="false" text="Outils">
         <items>
            <MenuItem mnemonicParsing="false" text="Ouvrir" onAction="#ouvrirFichier" />
            <MenuItem mnemonicParsing="false" text="Enregistrer" onAction="#sauvegarderFichier"/>
         </items>
      </Menu>
  </menus>
</MenuBar>
